CC = gcc
CFLAGS = -Wall -Wextra
NAME = program

UNITY = ./Unity/src/unity.c

algorithm:
	$(CC) $(UNITY) test_algorithm.c -g -o $(NAME) $(CFLAGS)

string:
	$(CC) $(UNITY) test_string.c -g -o $(NAME) $(CFLAGS)

list:
	$(CC) $(UNITY) test_list.c -g -o $(NAME) $(CFLAGS)

